<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".setupProfile">

    <EditText
        android:id="@+id/setupProfileHeight"
        android:layout_width="302dp"
        android:layout_height="41dp"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.697"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setupProfileWeight" />

    <TextView
        android:id="@+id/setupProfileGenreTextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inaltime (cm)"
        app:layout_constraintBottom_toTopOf="@+id/setupProfileHeight"
        app:layout_constraintStart_toStartOf="@+id/setupProfileHeight" />

    <EditText
        android:id="@+id/setupProfileWeight"
        android:layout_width="310dp"
        android:layout_height="40dp"
        android:layout_marginTop="36dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.762"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setupProfileAge" />

    <TextView
        android:id="@+id/setupProfileGenreTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Greutate (kg)"
        app:layout_constraintBottom_toTopOf="@+id/setupProfileWeight"
        app:layout_constraintStart_toStartOf="@+id/setupProfileWeight" />

    <TextView
        android:id="@+id/setupProfileTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Introduceti datele dvs."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/setupProfileName"
        android:layout_width="317dp"
        android:layout_height="47dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nume Prenume"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.744"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setupProfileTitle" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/setupProfileName"
        app:layout_constraintHorizontal_bias="0.794"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/setupProfileName"
        app:srcCompat="@drawable/user" />

    <EditText
        android:id="@+id/setupProfileBDate"
        android:layout_width="314dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.731"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setupProfileName"
        tools:text="dd/mm/yyyy" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="30dp"
        android:layout_height="33dp"
        app:layout_constraintEnd_toStartOf="@+id/setupProfileBDate"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/setupProfileBDate"
        app:srcCompat="@drawable/calendar" />

    <Spinner
        android:id="@+id/setupProfileGenre"
        android:layout_width="142dp"
        android:layout_height="35dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.263"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setupProfileBDate" />

    <TextView
        android:id="@+id/setupProfileGenreTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gen"
        app:layout_constraintBottom_toTopOf="@+id/setupProfileGenre"
        app:layout_constraintStart_toStartOf="@+id/setupProfileGenre" />

    <EditText
        android:id="@+id/setupProfileAge"
        android:layout_width="153dp"
        android:layout_height="37dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toEndOf="@+id/setupProfileGenre"
        app:layout_constraintTop_toBottomOf="@+id/setupProfileBDate" />

    <TextView
        android:id="@+id/setupProfileGenreTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Varsta"
        app:layout_constraintBottom_toTopOf="@+id/setupProfileAge"
        app:layout_constraintStart_toStartOf="@+id/setupProfileAge" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/setupProfileFinishBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="72dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/checked" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="36dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/setupProfileHeight"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/setupProfileHeight"
        app:srcCompat="@drawable/height" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        app:layout_constraintEnd_toStartOf="@+id/setupProfileWeight"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/setupProfileWeight"
        app:srcCompat="@drawable/weight" />

</androidx.constraintlayout.widget.ConstraintLayout>